<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="50700069"
    package="com.redbend.client" android:versionName="5.7.0.69">

    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
	<uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE"/>
	<uses-permission android:name="android.permission.RECEIVE_WAP_PUSH" />
 	<uses-permission android:name="android.permission.RECEIVE_SMS" />
 	<uses-permission android:name="android.permission.GET_TASKS" />
 	<uses-permission android:name="android.permission.GET_ACCOUNTS" />    
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.INSTALL_PACKAGES" />
	<uses-permission android:name="android.permission.DELETE_PACKAGES" />
	<uses-permission android:name="android.permission.REBOOT" />
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="com.redbend.client.permission.C2D_MESSAGE" />
	<uses-permission android:name="com.redbend.permission.EVENT_INTENT" />	  
	<uses-permission android:name="android.permission.ACCESS_CACHE_FILESYSTEM" />
	<uses-permission android:name="android.permission.BIND_DEVICE_ADMIN" />
	<uses-permission android:name="android.permission.WRITE_SETTINGS" />
	<uses-sdk android:minSdkVersion="14" android:targetSdkVersion="17"/>
 	
	 <permission android:name="com.redbend.client.permission.C2D_MESSAGE" 
        android:protectionLevel="signature" /> 
        
        
	<permission
        android:name="com.redbend.permission.EVENT_INTENT"
        android:description="@string/event_intent_permission_description"
        android:protectionLevel="signatureOrSystem" /> 
	 
	 
    <application android:label="@string/app_name" android:icon="@drawable/icon" 
        android:allowClearUserData="true"
        android:manageSpaceActivity = "com.redbend.client.ManageSpaceActivity"
        android:theme="@style/ActionBarTheme"
        android:persistent="true">
            
    <service android:name="com.redbend.client.BasicService" />
            
	<activity android:name="com.redbend.client.StartupActivity"
	        android:configChanges="orientation|screenSize|keyboardHidden"
	        android:icon="@drawable/icon"
	        android:label="@string/app_name"
	        android:theme="@android:style/Theme.NoDisplay">
         <intent-filter>
            <action android:name="android.intent.action.MAIN" />
           <category android:name="android.intent.category.LAUNCHER" />
          </intent-filter>
    </activity>     
 
	 <activity
	     android:name="com.redbend.client.SwmEnrollActivity"
	     android:icon="@drawable/icon"
	     android:label="@string/app_name" >
	     <intent-filter>
	         <action android:name="android.intent.action.MAIN" />
	     </intent-filter>
	     <intent-filter>
	         <action android:name="android.intent.action.VIEW" />

	         <category android:name="android.intent.category.DEFAULT" />
	         <category android:name="android.intent.category.BROWSABLE" />

	         <data	          
	             android:scheme="redbend"
	              />
	     </intent-filter>
	 </activity>


	<activity android:name="com.redbend.client.ManageSpaceActivity" 
	     android:configChanges="orientation|screenSize|keyboardHidden"   
	/>
	     
	<activity android:name="com.redbend.client.ui.LoadingActivity" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize|keyboardHidden"/>             
    <activity android:name="com.redbend.client.ui.ErrorHandler" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.uialerts.ConfirmUIAlert" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>
	<activity android:name="com.redbend.client.uialerts.InfoUIAlert" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>
	<activity android:name="com.redbend.client.uialerts.InputUIAlert" android:label="@string/app_name" android:icon="@drawable/icon" 
		android:windowSoftInputMode="adjustResize" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.ui.ScomoConfirm" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.ui.ScomoInstallConfirm" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize"/>    
    <activity android:name="com.redbend.client.ui.ScomoDownloadProgress" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.ui.ScomoDownloadInterrupted" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.ui.ScomoDownloadSuspend" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize"/>
	<activity android:name="com.redbend.client.ui.ScomoInstallProgress" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize" />
    <activity android:name="com.redbend.client.ui.ScomoInstallDone" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize" />
	<activity android:name="com.redbend.client.ui.BatteryLow" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize" />
	<activity android:name="com.redbend.client.ui.ScomoPostponeConfirm" android:icon="@drawable/icon" android:label="@string/scomo_title" android:configChanges="orientation|screenSize" />
    <activity android:name="com.redbend.client.ui.LawmoWipeResult" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.ui.LawmoLockResult" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.ui.LawmoUnLockResult" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>
    <activity android:name="com.redbend.client.ui.RecoveryReboot" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>
	
	<activity android:name="com.redbend.client.ui.InstallApk"
		android:configChanges="orientation|screenSize" android:icon="@drawable/icon" 
		android:label="@string/app_name" 
		android:theme="@android:style/Theme.NoDisplay" />
	
    <activity android:name="com.redbend.swm_common.ui.SetPasswordPolicyUi" android:label="@string/app_name" android:icon="@drawable/icon" android:configChanges="orientation|screenSize"/>

	<service android:name="com.redbend.client.ClientService" />

    	<receiver android:enabled="true" android:name="com.redbend.client.DmcSMSReceiver">     
    		<!-- emulator only -->
            <intent-filter>
    			<action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>
        <receiver android:enabled="true" android:name="com.redbend.client.DmcWapReceiver">
    		<!-- device only-->
    		<intent-filter>
    			<action android:name="android.provider.Telephony.WAP_PUSH_RECEIVED" />
                <data android:mimeType="*/*" />
            </intent-filter>
        </receiver>  

    	<receiver android:enabled="true" 
        		  android:name="com.redbend.client.DmcBootReceiver">
                <intent-filter>
                	<action android:name="android.intent.action.BOOT_COMPLETED" />
                </intent-filter>
        </receiver>
        
		<receiver android:enabled="true" android:name="com.redbend.swm_common.DmcDeviceAdminReceiver"
                android:permission="android.permission.BIND_DEVICE_ADMIN">
            <meta-data android:name="android.app.device_admin"
                       android:resource="@xml/dev_admin" />
            <intent-filter>
                <action android:name="android.app.action.DEVICE_ADMIN_ENABLED" />
            </intent-filter>
        </receiver>    
        <receiver android:name="ScomoAlarmReceiver"/>
        <receiver android:name="com.redbend.client.RBGcmReceiver" android:permission="com.google.android.c2dm.permission.SEND" >
		    <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />
                <action android:name="android.accounts.LOGIN_ACCOUNTS_CHANGED" />                
                <category android:name="com.redbend.client" />
            </intent-filter>
        </receiver>    
        
         <receiver
            android:name="com.redbend.client.DeviceUpdateReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="SwmClient.ENABLE_PERIODIC_CHECK_FOR_UPDATES" />
                <action android:name="SwmClient.DISABLE_PERIODIC_CHECK_FOR_UPDATES" />
                <action android:name="SwmClient.CHANGE_PERIODIC_CHECK_FOR_UPDATES" />
                <action android:name="SwmClient.CHECK_FOR_UPDATES_NOW" />
                <action android:name="SwmClient.START_APPLICATION" /> 
            </intent-filter>
        </receiver>
        
    </application>
</manifest>
